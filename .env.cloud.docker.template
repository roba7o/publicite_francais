# Docker environment for Cloud SQL connection
# Copy this to .env.cloud.docker and fill in your credentials
# NOTE: Use 127.0.0.1 when using --network host with docker run

ENVIRONMENT=development
POSTGRES_HOST=127.0.0.1
POSTGRES_PORT=5432
POSTGRES_DB=french_news_db
POSTGRES_USER=news_user
POSTGRES_PASSWORD=<your-cloud-sql-password>
